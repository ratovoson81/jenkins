{
  "name": "jenkins",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.2.0",
    "@headlessui/react": "^1.3.0",
    "@reduxjs/toolkit": "^1.6.0",
    "@testing-library/dom": "^8.1.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "jest-svg-transformer": "^1.0.0",
    "jest-transform-css": "^2.1.0",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-bnb-gallery": "^1.4.4",
    "react-dom": "^17.0.2",
    "react-loading": "^2.0.3",
    "react-modal": "^3.14.2",
    "react-photoswipe-gallery": "^1.3.6",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-toast-notifications": "^2.4.4",
    "react-top-loading-bar": "^2.0.1",
    "spinners-react": "^1.0.4",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --coverage --watchAll",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "jest-junit": "^12.2.0",
    "netlify-cli": "^6.6.1",
    "postcss": "^7.0.36",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.9"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "coverageThreshold": {
      "global": {}
    },
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "lcov",
      "json",
      "text",
      "clover",
      "cobertura"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.svg$": "jest-svg-transformer",
      "^.+\\.css$": "jest-transform-css"
    }
  },
  "jest-junit": {
    "outputDirectory": "output/coverage/junit",
    "outputName": "junit.xml",
    "usePathForSuiteName": "true"
  }
}
